{
  "expo": {
    "name": "weathersmileycam",
    "slug": "weathersmileycam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "React Navigation V5 + EXPO CLI + Weather App + Smiley Cam App",
    "githubUrl": "https://github.com/developer-joker/weathersmileycam"
  }
}
